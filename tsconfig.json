{
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true,
    "exclude": ["node_modules/**"],
    "typeRoots": ["node_modules/@types"],
    "compilerOptions": {
      "outDir": "lib",
      "declaration": true,
      "sourceMap": true
    }
  }
  